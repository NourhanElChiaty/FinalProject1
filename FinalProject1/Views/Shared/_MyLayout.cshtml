<!DOCTYPE html>
<html>
<head>
    <title>@Page.Title</title>
    @RenderSection("head", required: false)
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="webthemez">
    <title>Rolls Car Rental Bootstrap Website Template - WebThemez</title>
    <!-- core CSS -->
    <link href=@Url.Content("/Content/css/bootstrap.min.css") rel="stylesheet">
    <link href=@Url.Content("/Content/css/font-awesome.min.css") rel="stylesheet">
    <link href=@Url.Content("/Content/css/animate.min.css") rel="stylesheet">
    <link href=@Url.Content("/Content/css/owl.carousel.css") rel="stylesheet">
    <link href=@Url.Content("/Content/css/owl.transitions.css") rel="stylesheet">
    <link href=@Url.Content("/Content/css/prettyPhoto.css") rel="stylesheet">
    <link href=@Url.Content("/Content/css/magnific-popup.css") rel="stylesheet">
    <link href=@Url.Content("/Content/css/gallery-1.css") rel="stylesheet">
    <link href=@Url.Content("/Content/css/styles.css") rel="stylesheet">
    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->
    <link rel="shortcut icon" href=@Url.Content("~/Content/images/ico/favicon.ico")>
</head>
<body>
    @RenderBody()
    <header id="header">
        <nav id="main-nav" class="navbar navbar-default navbar-fixed-top" role="banner">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <i class="fa fa-car" aria-hidden="true"></i>
                    <div class="logoSection">
                        <a class="navbar-brand" href="index.html">Rolls</a>
                        <span class="caption">Car Rental</span>
                    </div>
                </div>

                <div class="collapse navbar-collapse navbar-right">
                    <ul class="nav navbar-nav">
                        <li class="scroll active">@Html.ActionLink("Home","Index","Home")</li>
                        <li>@Html.ActionLink("Rent Car", "RentCarForm", "Clients")</li>
                        <li class="scroll"><a href="#about">About Us</a></li>
                        <li class="scroll"><a href="#portfolio">Gallery</a></li>
                        <li class="scroll"><a href="#our-team">Team</a></li>
                        <li class="scroll"><a href="#pricing">Pricing</a></li>
                        <li class="scroll"><a href="#contact-us">Contact</a></li>
                    </ul>
                </div>
            </div><!--/.container-->
        </nav><!--/nav-->
    </header><!--/header-->
    <footer id="footer">
        <div class="container">
            <div class="row">
                <div class="col-sm-6">
                    &copy; 2018 Company Name.
                </div>
                <div class="col-sm-6">
                    <ul class="social-icons">
                        <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                        <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                        <li><a href="#"><i class="fa fa-youtube"></i></a></li>
                        <li><a href="#"><i class="fa fa-github"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </footer><!--/#footer-->
    <script src=@Url.Content("/Content/js/jquery.js")></script>
    <script src=@Url.Content("/Content/js/bootstrap.min.js")></script>
    <script src="http://maps.google.com/maps/api/js?sensor=true"></script>
    <script src=@Url.Content("/Content/js/owl.carousel.min.js")></script>
    <script src=@Url.Content("/Content/js/mousescroll.js")></script>
    <script src=@Url.Content("/Content/js/smoothscroll.js")></script>
    <script src=@Url.Content("/Content/js/jquery.prettyPhoto.js")></script>
    <script src=@Url.Content("/Content/js/jquery.inview.min.js")></script>
    <script src=@Url.Content("/Content/js/wow.min.js")></script>
    <script src=@Url.Content("/Content/contact/jqBootstrapValidation.js")></script>
    <script src=@Url.Content("/Content/contact/contact_me.js")></script>

    <script type="text/javascript" src=@Url.Content("/Content/js/jquery.isotope.min.js")></script><!-- Gallery Filter -->
    <script type="text/javascript" src=@Url.Content("/Content/js/jquery.magnific-popup.min.js")></script><!-- Gallery Popup -->

    <script src=@Url.Content("/Content/js/custom-scripts.js")></script>

</body>
</html>
